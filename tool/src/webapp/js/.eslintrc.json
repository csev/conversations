{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "Vue": "readonly",
        "RichText": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "max-len": "off",
        "require-jsdoc": "off"
    }
}
